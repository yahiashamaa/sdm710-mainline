# SDM670-specific drivers
CONFIG_PINCTRL_SDM670=y
CONFIG_PINCTRL_SDM670_LPASS_LPI=m
CONFIG_INTERCONNECT_QCOM_SDM670=y
CONFIG_SND_SOC_SDM660_INT=m

# Use simplefb instead of drmfb in initramfs so fde is simpler
CONFIG_SDM_DISPCC_845=m
CONFIG_DRM_MSM=m

# Pixel 3a/3a XL panel driver
CONFIG_DRM_PANEL_SAMSUNG_S6E3FA7=m
CONFIG_DRM_PANEL_SAMSUNG_SOFEF00_BONITO=m

# Pixel 3a haptics
CONFIG_INPUT_DRV2624_HAPTICS=y

# Pixel 3a NFC
CONFIG_NFC_NXP_NCI=m
CONFIG_NFC_NXP_NCI_I2C=m

# Pixel 3a modemsmem
CONFIG_QCOM_MODEMSMEM=y

# Pixel 3a sound
CONFIG_SND_SOC_CS35L36=m
CONFIG_SND_SOC_MSM8916_WCD_ANALOG=m
CONFIG_SND_SOC_MSM8916_WCD_DIGITAL=m

# Pixel 3a camera
CONFIG_VIDEO_IMX355=m

# Branding
CONFIG_LOCALVERSION="-sdm670"

# Enable firmware compression
CONFIG_FW_LOADER_COMPRESS=y
CONFIG_FW_LOADER_COMPRESS_ZSTD=y

# No need for ZBOOT
CONFIG_EFI_ZBOOT=n

# Irrelevant subsystems
CONFIG_ACPI=n
CONFIG_EFI=n
CONFIG_PCI=n

# Irrelevant platforms
CONFIG_ARCH_SPARX5=n
CONFIG_ARCH_MA35=n
CONFIG_ARCH_REALTEK=n
CONFIG_ARCH_STM32=n

# Irrelevant drivers
